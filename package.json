{
  "name": "t--",
  "version": "1.0.0",
  "description": "Minimalistic testing",
  "main": "t--.js",
  "scripts": {
    "debug:node": "concurrently 'rollup -wc rollup.test.js' 'nodemon --inspect --debug-brk test.js'",
    "debug:browser": "concurrently 'rollup -wc rollup.test.js' 'budo test.js --open --live'",
    "test": "rollup -c rollup.test.js; node test.js"
  },
  "keywords": [
    "testing"
  ],
  "author": "Kevin Jahns <kevin.jahns@rwth-aachen.de>",
  "license": "MIT",
  "devDependencies": {
    "budo": "^10.0.3",
    "chance": "^1.0.8",
    "concurrently": "^3.4.0",
    "debug": "^2.6.6",
    "diff": "^3.2.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-multi-entry": "^2.0.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2"
  },
  "dependencies": {
    "meow": "^3.7.0"
  }
}
